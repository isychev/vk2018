{
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "fetch": true,
    "URLSearchParams": true
  },
   "rules": {
    "import/resolver":0,
    "import/no-unresolved":0,
    "import/extensions":0,
    "import/no-extraneous-dependencies": 0,
    "prettier/prettier": [
      "error", {
      "singleQuote": true,
        "trailingComma": "all"
      }
    ]
  }
}
